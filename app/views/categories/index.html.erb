<h1 class="text-center">Categories</h1>

<% @categories.each do |category| %>
  <ul class="listing">
    <div class="row">
      <div class="col-md-4 col-md-offset-4 well">
        <li class="article-title text-center story-title">
          <%= link_to "#{category.name}", category_path(category) %>
        </li>
      </div>
    </div>
  </ul>
<% end %>